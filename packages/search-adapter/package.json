{
  "name": "@empathyco/x-adapter",
  "version": "6.1.0",
  "description": "A search client for the Empathy search API ",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "tsc --project tsconfig.cjs.json && tsc --project tsconfig.esm.json && npm run gen:docs && npm pack",
    "build:watch": "tsc --watch",
    "gen:model-docs": "api-extractor run -l",
    "gen:typescript-docs": "api-documenter markdown -i report -o docs",
    "gen:docs": "npm run gen:model-docs && npm run gen:typescript-docs",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "link": "npm link @empathy/search-types && npm link @empathy/logger"
  },
  "repository": {
    "type": "git",
    "url": "git+https://cbn-javieri@bitbucket.org/colbenson/search-adapter.git"
  },
  "keywords": [
    "search",
    "adapter",
    "client",
    "fetch"
  ],
  "homepage": "https://bitbucket.org/colbenson/search-adapter#readme",
  "dependencies": {
    "@empathyco/x-get-safe-property-chain": "^1.2.0",
    "@empathyco/x-logger": "^1.1.0",
    "@empathyco/x-types": "^9.0.0",
    "@empathyco/x-storage-service": "^1.3.0",
    "@empathyco/x-deep-merge": "^1.2.0",
    "inversify": "~5.0.1",
    "reflect-metadata": "~0.1.13",
    "tslib": "~1.11.1"
  },
  "devDependencies": {
    "@empathyco/x-jest-utils": "^1.3.0",
    "@microsoft/api-documenter": "~7.5.8",
    "@microsoft/api-extractor": "~7.5.6",
    "@types/jest": "~24.0.13",
    "@types/node": "~12.7.5",
    "jest": "~24.7.1",
    "ts-jest": "~24.0.2",
    "typescript": "~3.8.3"
  },
  "files": [
    "dist",
    "types",
    "report"
  ],
  "publishConfig": {
    "access": "public"
  }
}
