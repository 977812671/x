{
  "name": "@empathy/search-adapter",
  "version": "4.0.0",
  "description": "A search client for the Empathy search API ",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "tsc && tsc --project tsconfig.esm.json",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "tslint --project tsconfig.json",
    "link": "npm link @empathy/search-types && npm link @empathy/logger"
  },
  "repository": {
    "type": "git",
    "url": "git+https://cbn-javieri@bitbucket.org/colbenson/search-adapter.git"
  },
  "keywords": [
    "search",
    "adapter",
    "client",
    "fetch"
  ],
  "homepage": "https://bitbucket.org/colbenson/search-adapter#readme",
  "dependencies": {
    "@empathy/get-safe-property-chain": "^1.2.0",
    "@empathy/logger": "^1.1.0",
    "@empathy/search-types": "^5.0.0",
    "@empathy/storage-service": "^1.3.0",
    "@empathybroker/deep-merge": "^1.2.0",
    "inversify": "~5.0.1",
    "reflect-metadata": "~0.1.13",
    "tslib": "~1.11.1"
  },
  "devDependencies": {
    "@empathy/jest-utils": "^1.1.0",
    "@empathybroker/eb-tslint": "0.0.15",
    "@types/jest": "~24.0.12",
    "@types/node": "^12.7.5",
    "jest": "~24.7.1",
    "ts-jest": "~24.0.2",
    "tslint": "~5.16.0",
    "typescript": "~3.8.3"
  },
  "files": [
    "dist",
    "types"
  ]
}
