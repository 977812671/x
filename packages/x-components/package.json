{
  "name": "@empathy/x-components",
  "main": "./core/index.js",
  "module": "./core/index.js",
  "types": "./core/index.d.ts",
  "version": "0.0.1",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "ts-node --project ./build/tsconfig.json ./build/build.ts && npm pack",
    "build:website": "vue-cli-service build",
    "gen:docs": "npm run gen:typescript-docs && npm run gen:component-docs",
    "gen:typescript-docs": "api-documenter markdown --input-folder temp --output-folder docs",
    "gen:component-docs": "vue-docgen",
    "test:unit": "vue-cli-service test:unit",
    "test:unit-coverage": "vue-cli-service test:unit --coverage",
    "test:unit-watch": "vue-cli-service test:unit --watch",
    "test:e2e": "vue-cli-service test:e2e",
    "test:e2e:ci": "vue-cli-service test:e2e --headless --browser chrome",
    "test": "npm run test:unit && npm run test:e2e:ci",
    "cypress:open": "cypress open",
    "lint": "vue-cli-service lint build src tests",
    "commit": "git-cz",
    "release": "standard-version --no-verify",
    "release:alpha": "standard-version --no-verify --prerelease alpha"
  },
  "dependencies": {
    "@empathy/logger": "^1.0.0",
    "@empathy/search-adapter": "^4.0.0-alpha.2",
    "@empathy/search-types": "^5.0.0-alpha.3",
    "@empathybroker/deep-merge": "^1.1.2",
    "reflect-metadata": "~0.1.13",
    "rxjs": "~6.5.4",
    "rxjs-compat": "~6.5.4",
    "tslib": "~1.10.0",
    "vue": "~2.6.10",
    "vue-class-component": "~7.1.0",
    "vue-property-decorator": "~8.3.0",
    "vue-runtime-helpers": "~1.1.2",
    "vuex": "~3.1.2"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "~7.7.12",
    "@microsoft/api-extractor": "~7.7.8",
    "@rollup/plugin-commonjs": "~11.0.1",
    "@types/autoprefixer": "~9.6.1",
    "@types/jest": "~24.0.19",
    "@typescript-eslint/eslint-plugin": "~2.22.0",
    "@typescript-eslint/parser": "~2.22.0",
    "@vue/cli-plugin-e2e-cypress": "~4.1.0",
    "@vue/cli-plugin-eslint": "~4.1.0",
    "@vue/cli-plugin-typescript": "~4.1.0",
    "@vue/cli-plugin-unit-jest": "~4.1.0",
    "@vue/cli-plugin-vuex": "~4.1.0",
    "@vue/cli-service": "~4.1.0",
    "@vue/eslint-config-prettier": "~5.1.0",
    "@vue/eslint-config-typescript": "~4.0.0",
    "@vue/test-utils": "~1.0.0-beta.32",
    "autoprefixer": "~9.7.4",
    "commitizen": "~4.0.3",
    "cz-conventional-changelog": "~3.1.0",
    "eslint": "~5.16.0",
    "eslint-plugin-chai-friendly": "~0.5.0",
    "eslint-plugin-cypress": "~2.9.0",
    "eslint-plugin-import": "~2.20.1",
    "eslint-plugin-jest": "~23.7.0",
    "eslint-plugin-jsdoc": "~22.1.0",
    "eslint-plugin-prettier": "~3.1.2",
    "eslint-plugin-tsdoc": "~0.2.3",
    "eslint-plugin-vue": "~5.2.3",
    "lint-staged": "~10.0.7",
    "prettier": "~1.19.1",
    "rollup": "~1.31.0",
    "rollup-plugin-delete": "~1.2.0",
    "rollup-plugin-postcss": "~2.0.3",
    "rollup-plugin-scss": "~2.1.0",
    "rollup-plugin-sourcemaps": "~0.5.0",
    "rollup-plugin-typescript2": "~0.25.3",
    "rollup-plugin-vue": "~5.1.5",
    "sass": "~1.24.0",
    "sass-loader": "~8.0.0",
    "standard-version": "~7.1.0",
    "ts-node": "~8.6.2",
    "typescript": "~3.7.4",
    "vue-docgen-cli": "~4.13.1",
    "vue-template-compiler": "~2.6.10"
  },
  "files": [
    "dist/**/*",
    "types/**/*",
    "**/*.js",
    "**/*.js.map",
    "**/*.d.ts",
    "!/build/**",
    "!/public/**",
    "!/src/**",
    "!/tests/**"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "./**/*.{vue,js,jsx,ts,tsx}": [
      "npm run lint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
