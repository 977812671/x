{
  "name": "@empathybroker/deep-merge",
  "version": "1.0.5",
  "description": "Clones an array of objects into another object.",
  "keywords": [
    "deep",
    "merge",
    "assign",
    "clone",
    "copy"
  ],
  "main": "dist/deep-merge.js",
  "typings": "dist/deep-merge.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/colbenson/eb-deep-merge.git"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "build": "webpack --mode production",
    "build:local": "webpack --mode production && npm pack",
    "clean": "rimraf dist && rimraf coverage",
    "format": "prettier --write \"{src,__tests__}/**/*.ts\" --single-quote --trailing-comma es5",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "prebuild": "npm run clean && npm run lint && echo Using TypeScript && tsc --version",
    "test": "jest"
  },
  "devDependencies": {
    "@empathybroker/eb-tslint": "0.0.8",
    "@types/jest": "^22.2.3",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "cpx": "^1.5.0",
    "jest": "^22.4.3",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.3",
    "ts-loader": "^4.3.1",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1",
    "webpack": "^4.10.2",
    "webpack-cli": "^3.0.1"
  }
}
